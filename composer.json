{
    "name": "simplesamlphp/simplesamlphp-module-saml2debug",
    "description": "A module to debug encoding and decoding of SAML message for both the HTTP-redirect and HTTP-POST bindings.",
    "type": "simplesamlphp-module",
    "license": "LGPL-2.1-or-later",
    "keywords": [ "simplesamlphp", "modules" ],
    "authors": [
        {
            "name": "Andreas Ã…kre Solberg",
            "email": "andreas.solberg@uninett.no"
        },
        {
            "name": "Olav Morken",
            "email": "olav.morken@uninett.no"
        }
    ],
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\saml2debug\\": "src/"
        }
    },
    "require": {
        "php": "^8.3",
        "ext-zlib": "*",

        "simplesamlphp/simplesamlphp": "^2.5@dev",
        "symfony/http-foundation": "^7.4"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^1.11"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "simplesamlphp/composer-module-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "simplesamlphp/composer-xmlprovider-installer": true
        }
    }
}
